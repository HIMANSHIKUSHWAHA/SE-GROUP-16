{
  "dependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start:backend": "cd backend && npm start",
    "start:ui": "cd ui && npm start",
    "start": "concurrently \"npm run start:backend\" \"npm run start:ui\"",
    "build": "cd ui && npm install && cd ../backend && npm install && cd .."
  }
}
